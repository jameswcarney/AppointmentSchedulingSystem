<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="597.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportsController">
   <children>
      <TableView fx:id="ByTypeTable" layoutX="14.0" layoutY="34.0" prefHeight="200.0" prefWidth="252.0">
        <columns>
          <TableColumn fx:id="ByTypeMonthColumn" prefWidth="75.0" text="Month" />
          <TableColumn fx:id="ByTypeTypeColumn" prefWidth="100.0" text="Type" />
            <TableColumn fx:id="ByTypeCountColumn" prefWidth="75.0" text="Count" />
        </columns>
      </TableView>
      <Label fx:id="ByTypeLabel" layoutX="26.0" layoutY="9.0" text="Appointments by Type and month">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="ScheduleTable" layoutX="13.0" layoutY="324.0" prefHeight="200.0" prefWidth="675.0">
        <columns>
          <TableColumn fx:id="ScheduleContactIdColumn" prefWidth="75.0" text="Contact ID" />
          <TableColumn fx:id="ScheduleAppointmentIdColumn" prefWidth="103.0" text="Appointment ID" />
            <TableColumn fx:id="ScheduleTitleColumn" prefWidth="72.0" text="Title" />
            <TableColumn fx:id="ScheduleTypeColumn" prefWidth="68.0" text="Type" />
            <TableColumn fx:id="ScheduleDescriptionColumn" prefWidth="104.0" text="Description" />
            <TableColumn fx:id="ScheduleStartColumn" prefWidth="83.0" text="Start" />
            <TableColumn fx:id="ScheduleEndColumn" prefWidth="81.0" text="End" />
            <TableColumn fx:id="ScheduleCustomerIdColumn" prefWidth="86.0" text="Customer ID" />
        </columns>
      </TableView>
      <Label fx:id="ScheduleByContact" layoutX="235.0" layoutY="299.0" text="Schedules Organized By Contact ID">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Button fx:id="cancelButton" layoutX="634.0" layoutY="558.0" mnemonicParsing="false" onAction="#onCancelButtonClicked" text="Cancel" />
      <TableView fx:id="customersByCountryTable" layoutX="484.0" layoutY="34.0" prefHeight="200.0" prefWidth="202.0">
        <columns>
          <TableColumn fx:id="cbcCountryColumn" prefWidth="125.0" text="Country" />
          <TableColumn fx:id="cbcCustomersColumn" prefWidth="75.0" text="Customers" />
        </columns>
      </TableView>
      <Label fx:id="CustomersByCountry" layoutX="513.0" layoutY="9.0" text="Customers by Country">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
